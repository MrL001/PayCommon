<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hntyy.mapper.mjjzxyh.SchoolMapper">
    <select id="findAll" resultType="com.hntyy.entity.mjjzxyh.SchoolEntity">
        SELECT * FROM school_tb
    </select>

    <select id="findSchoolById" resultType="com.hntyy.entity.mjjzxyh.SchoolEntity">
        SELECT * FROM school_tb where school_id=#{schoolId}
    </select>
</mapper>